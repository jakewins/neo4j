.data-console
  .row-fluid(ng-repeat='interaction in interactions')
    .span12
      pre.data-console-statement {{ interaction.statement }}
      pre.data-console-result    {{ interaction.result }}
  .data-console-dash
    .data-console-input-wrap
      .btn.btn-primary.data-console-exec(ng-click='execute()') Execute &raquo;
      .code-mirror-wrap
        textarea(ui-codemirror="{theme:'eclipse',mode:'cypher'}", ng-model="statement")